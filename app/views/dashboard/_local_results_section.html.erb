<section>
  <h3 class="text-xs font-semibold uppercase tracking-wider text-woodsmoke-400 mb-3">
    Results
  </h3>
  <div class="flex flex-col gap-3">
    <% if @local_results[:artists].any? %>
      <% @local_results[:artists].each do |artist| %>
        <%= render "dashboard/artist_row_local", artist: artist %>
      <% end %>
    <% end %>

    <%= render "dashboard/external_results_section" %>
  </div>
</section>
